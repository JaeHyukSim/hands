<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper
	namespace="com.bangkoklab.data.repository.mapper.ContractMapper">
	
	<select id="getHandyByContractId" parameterType="String" resultType="String">
		SELECT handy
		FROM contract
		WHERE contract_id = #{contract_id}
	</select>
	
</mapper>